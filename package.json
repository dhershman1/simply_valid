{
  "name": "simply_valid",
  "version": "3.0.1",
  "description": "a simple data driven validation utility library",
  "main": "dist/simply_valid.umd.js",
  "module": "esm/index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "prepare": "rollup -c",
    "compile:has": "rollup -i src/has/tests.js -o src/tests/dist/has.cjs.js -f cjs -g tape",
    "compile:is": "rollup -i src/is/tests.js -o src/tests/dist/is.cjs.js -f cjs -g tape",
    "compile:internals": "rollup -i src/_internals/tests.js -o src/tests/dist/internals.cjs.js -f cjs -g tape",
    "compile:meets": "rollup -i src/meets/tests.js -o src/tests/dist/meets.cjs.js -f cjs -g tape",
    "broken:multi": "rollup -i src/multi/tests.js -o src/tests/dist/multi.cjs.js -f cjs -g tape",
    "compile:no": "rollup -i src/no/tests.js -o src/tests/dist/no.cjs.js -f cjs -g tape",
    "build:all": "npm-run-all --parallel compile:*",
    "test:methods": "npm run build:all && tape src/**/*.cjs.js | tap-spec",
    "test:all": "tape src/tests/{dist/*.cjs.js,tests.js} | tap-spec",
    "test:main": "rollup -c && tape src/tests/tests.js | tap-spec",
    "test:no": "tape tests/dist/no.cjs.js | tap-spec",
    "test:multi": "npm run build:all && tape tests/dist/multi.cjs.js | tap-spec",
    "test:internals": "npm run compile:internals && tape src/tests/dist/internals.cjs.js | tap-spec",
    "test": "rollup -c && npm-run-all --parallel compile:* && npm run test:all",
    "lint": "eslint ./src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dhershman1/simply_valid.git"
  },
  "keywords": [
    "validation",
    "simple",
    "data",
    "simply",
    "valid",
    "frontend",
    "JS"
  ],
  "author": "Dustin Hershman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dhershman1/simply_valid/issues"
  },
  "homepage": "https://github.com/dhershman1/simply_valid",
  "devDependencies": {
    "npm-run-all": "4.1.1",
    "rollup": "0.50.0",
    "rollup-plugin-buble": "0.16.0",
    "rollup-plugin-uglify": "2.0.1",
    "tap-spec": "4.1.1",
    "tape": "4.8.0"
  }
}
