{
  "name": "simply_valid",
  "version": "2.2.1",
  "description": "a simple data driven validation utility library",
  "main": "dist/simply_valid.umd.js",
  "module": "src/index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "prepublish": "rollup -c && rollup --config rollup.config.module.js",
    "compile:has": "rollup -i tests/has.js -o tests/dist/has.cjs.js -f cjs -g tape",
    "compile:is": "rollup -i tests/is.js -o tests/dist/is.cjs.js -f cjs -g tape",
    "compile:match": "rollup -i tests/match.js -o tests/dist/match.cjs.js -f cjs -g tape",
    "compile:meets": "rollup -i tests/meets.js -o tests/dist/meets.cjs.js -f cjs -g tape",
    "compile:multi": "rollup -i tests/multi.js -o tests/dist/multi.cjs.js -f cjs -g tape",
    "compile:no": "rollup -i tests/no.js -o tests/dist/no.cjs.js -f cjs -g tape",
    "build:all": "npm-run-all --parallel compile:*",
    "test:methods": "npm run build:all && tape tests/dist/*.cjs.js | tap-spec",
    "test:multi": "npm run build:all && tape tests/dist/multi.cjs.js | tap-spec",
    "test": "rollup -c && tape tests/tests.js | tap-spec"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dhershman1/simply_valid.git"
  },
  "keywords": [
    "validation",
    "simple",
    "data",
    "simply",
    "valid",
    "frontend",
    "JS"
  ],
  "author": "Dustin Hershman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dhershman1/simply_valid/issues"
  },
  "homepage": "https://github.com/dhershman1/simply_valid",
  "devDependencies": {
    "rollup": "^0.41.6",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-uglify": "^2.0.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3"
  }
}
