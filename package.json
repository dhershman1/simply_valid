{
  "name": "simply_valid",
  "version": "3.2.1",
  "description": "a simple data driven validation utility library",
  "main": "main/index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "prepare": "webpack",
    "test": "ava",
    "clean": "node ./scripts/clean.js",
    "postpublish": "npm run clean",
    "lint": "eslint ./src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dhershman1/simply_valid.git"
  },
  "keywords": [
    "validation",
    "simple",
    "data",
    "simply",
    "valid",
    "frontend",
    "JS"
  ],
  "author": "Dustin Hershman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dhershman1/simply_valid/issues"
  },
  "homepage": "https://github.com/dhershman1/simply_valid",
  "devDependencies": {
    "ava": "^0.24.0",
    "babel": "^6.23.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "del": "^3.0.0",
    "eslint": "^4.14.0",
    "globby": "^7.1.1",
    "jsdoc-to-markdown": "^3.0.3",
    "webpack": "^3.10.0"
  },
  "ava": {
    "files": [
      "src/**/tests.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "concurrency": 5,
    "require": [
      "babel-register"
    ],
    "babel": {
      "presets": [
        "env",
        "@ava/stage-4",
        "@ava/transform-test-files"
      ]
    }
  }
}
